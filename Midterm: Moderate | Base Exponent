package Practice;
import java.util.*;

public class Main {
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.print("Enter base: ");
		int base = sc.nextInt();
		
		System.out.print("Enter exponent: ");
		int exponent = sc.nextInt();
		
		System.out.println(Math.pow(base, exponent));
	}
}
