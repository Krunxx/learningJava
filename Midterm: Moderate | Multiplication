package Practice;
import java.util.*;

public class Main {
	
	public static void main(String[] args) {
		
		Scanner sc  = new Scanner(System.in);
		
		//declare int variable num and sum
		int num = 0, sum =0;
		
		//ask user for input
		System.out.print("Enter number: ");
		num = sc.nextInt();
		sc.close();
		
		//Purpose: Display the Multiplying input
		for(int i=1; i <= 10; i++)
    {	
			sum = num * i;
			System.out.println(num + " * " + i + " = " + sum);
		}
	}
}
